mixin head()
  meta(charset="utf-8")
  meta(name="viewport", content="width=device-width, initial-scale=1.0")
  meta(name="description", content="#{siteDescription}")
  meta(name="author", content="#{siteAuthor}")

  title= siteTitle
  link(href="http://fonts.googleapis.com/css?family=Nunito:400,700,300", rel="stylesheet")
  link(href="/css/sticky-footer-navbar.css", rel="stylesheet")
  link(href="/css/cytube.css", rel="stylesheet")
  link(href="/css/eqtv.css", rel="stylesheet")

  link(rel="shortcut icon", href="/img/favicon.ico")
  link(rel="apple-touch-icon", href="/img/apple-touch-icon.png")
  link(rel="desktop-app-icon", href="/img/desktop-app-icon-256.png")
  link(rel="icon", type="image/png", href="/img/desktop-app-icon-256.png", sizes="192x192")

  meta(name="msapplication-TileColor", content="#98d61e")
  meta(name="msapplication-TileImage", content="/img/mstile-144x144.png")
  meta(name="msapplication-config", content="/img/browserconfig.xml")
  
  meta(name="theme-color", content="#98d61e")

  //link(id="usertheme", href="/css/themes/slate.css", rel="stylesheet")
